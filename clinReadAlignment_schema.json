              "null"
            ]
          },
          "alignmentTool": {
            "type": [
              "string",
              "null"
            ]
          },
          "referenceGenome": {
            "enum": [
              "GRCh38 / hg38",
              "GRCh37 / hg19"
            ],
            "type": [
              "string",
              "null"
            ]
          },
          "pairedEnd": {
            "type": [
              "boolean",
              "null"
            ]
          },
          "insertSize": {
            "type": [
              "integer",
              "null"
            ]
          },
          "SequencingStrategy": {
            "type": [
              "string",
              "null"
            ]
          },
          "SequencerId": {
            "type": [
              "string",
              "null"
            ]
          },
          "runId": {
            "type": [
              "string",
              "null"
            ]
          },
          "runDate": {
            "type": [
              "string",
              "null"
            ]
          },
          "totalReadsCount": {
            "type": [
              "integer",
              "null"
            ]
          },
          "readSupplementary": {
            "type": [
              "integer",
              "null"
            ]
          },
          "readDuplicates": {
            "type": [
              "integer",
              "null"
            ]
          },
          "readMapped": {
            "type": [
              "integer",
              "null"
            ]
          },
          "readProperlyPaired": {
            "type": [
              "integer",
              "null"
            ]
          },
          "Contamination_verifyBamId": {
            "type": [
              "number",
              "null"
            ]
          },
          "Duplication_%pcr": {
            "type": [
              "number",
              "null"
            ]
          }
